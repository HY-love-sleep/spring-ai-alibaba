hexagon "check state" as condition2<<Condition>>
"__START__" -down-> "sensitive"
"answer" -down-> "__END__"
"sensitive" .down.> "condition1"
"condition1" .down.> "clft": "no"
'"sensitive" .down.> "clft": "no"
"condition1" .down.> "answer": "yes"
'"sensitive" .down.> "answer": "yes"
"clft" -down-> "human"
"human" .down.> "condition2"
"condition2" .down.> "saveTool"
'"human" .down.> "saveTool"
"condition2" .down.> "clft"
'"human" .down.> "clft"
